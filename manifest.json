{
    "manifest_version": 2,
    "name": "Chromeless YouTube",
    "description": "Pops open a chromeless youtube player (similar to Hulu's popout player).",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs"
    ]
}
